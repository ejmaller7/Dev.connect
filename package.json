{
  "name": "dev.connect",
  "version": "1.0.0",
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "cd client && npm install --include=dev && cd ../server && npm install --include=dev",
    "build": "cd client && npm run build && cd ../server && npm run build",
    "seed": "cd server && npm run seed",
    "client:dev": "cd client && npm run dev",
    "start": "concurrently \"npm run start-client\" \"npm run start-server\"",
    "start-client": "cd client && npm run dev",
    "start-server": "cd server && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ejmaller7/Dev.connect.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ejmaller7/Dev.connect/issues"
  },
  "homepage": "https://github.com/ejmaller7/Dev.connect#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "react-toastify": "^11.0.5"
  }
}
